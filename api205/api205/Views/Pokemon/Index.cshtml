@model IEnumerable<api205.Models.PokemonInfo>

    <table>
        <thead>
            <tr>
                <th>
                    Nombre
                </th>
                <th>
                    URL
                </th>
               
            </tr>
        </thead>
    <tbody>
        @foreach (var pokemon in Model)
        {   
            <tr>
                <td>
                    @pokemon.Name
                </td>
                <td>
                    <img src="@pokemon.ImageUrl" alt="Imagen de @pokemon.Name"/>
          
                </td>
             
            </tr>
            
        }
    </tbody>
    </table>
    